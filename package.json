{
  "name": "express-prisma-architecture",
  "version": "1.0.0",
  "description": "Business logic and data access layer architecture",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon server.ts",
    "migrate": "npx prisma migrate dev --name init",
    "build": "tsc --build",
    "start": "node dist/server.js",
    "dev-js": "nodemon --exitcrash dist/server.js",
    "dev-prod": "tsc --build && nodemon --exitcrash dist/server.js",
    "generate": "npx prisma db pull && npx prisma generate",
    "check": "tsc --noEmit"
  },
  "keywords": [],
  "dependencies": {
    "@prisma/client": "^5.18.0",
    "acorn": "^8.12.1",
    "axios": "^1.7.4",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "chalk": "^5.3.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "helmet": "^7.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "moment-timezone": "^0.5.45",
    "multer": "^1.4.5-lts.1",
    "node-mailer": "^0.1.1",
    "node-schedule": "^2.1.1",
    "nodemon": "^3.1.4",
    "reflect-metadata": "^0.2.2",
    "uuidv4": "^6.2.13",
    "winston": "^3.14.1"
  },
  "devDependencies": {
    "@types/acorn": "^6.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^22.3.0",
    "@types/node-schedule": "^2.1.7",
    "@types/nodemailer": "^6.4.15",
    "prisma": "^5.18.0",
    "typescript": "^5.5.4"
  },
